{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.31",
        "doctrine/dbal": "2.5.12",
        "laravel/socialite": "2.0.21",
        "guzzlehttp/guzzle": "6.2.3",
        "laravelcollective/html":"5.3.1",
        "consoletvs/charts": "4.5.1",
        "fzaninotto/faker": "1.6.0",
        "khill/lavacharts" : "3.0.*",
        "maatwebsite/excel": "2.1.17",
        "predis/predis": "1.1.1",
        "barryvdh/laravel-dompdf": "0.8.0",
        "torann/database-backup": "0.1.0",
        "mariuzzo/laravel-js-localization": "^1.4",
        "yajra/laravel-datatables-oracle": "^6.0",
        "ext-gd": "*"
    },
    "require-dev": {
        "framgia/fauth-php": "dev-develop",
        "fzaninotto/faker": "1.6.0",
        "mockery/mockery": "0.9.9",
        "phpunit/phpunit": "5.7.19",
        "symfony/css-selector": "3.1.10",
        "symfony/dom-crawler": "3.1.10",
        "barryvdh/laravel-debugbar": "~2.4"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Framgia\\Fauth\\": "vendor/framgia/fauth-php/src/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
